<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minWidth="1041.0" prefHeight="400.0" prefWidth="1041.0" xmlns="http://javafx.com/javafx/8.0.112"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.sample.UnitController">
    <children>
        <fx:include source="./menu.fxml"/>
        <Label layoutX="427.0" layoutY="49.0" text="Faire une demande de réquisition  !"/>


        <Label layoutX="380.0" layoutY="128.0" text="Nom de l'unité :"/>
        <TextField fx:id="unitName" layoutX="522.0" layoutY="123.0"/>
        <Label layoutX="380.0" layoutY="182.0" text="Sigle de l'unité :"/>
        <TextField fx:id="unitSigle" layoutX="522.0" layoutY="177.0"/>
        <Label layoutX="380.0" layoutY="235.0" text="Mail :"/>
        <TextField fx:id="unitMail" layoutX="522.0" layoutY="230.0"/>


        <Button fx:id="dismissButton" layoutX="573.0" layoutY="425.0" mnemonicParsing="false"
                onAction="#handleDismissButton" text="Retour">
            <font>
                <Font size="12.0"/>
            </font>
        </Button>

        <Button fx:id="validationButton" layoutX="494.0" layoutY="425.0" mnemonicParsing="false"
                onAction="#handleValidationButton" text="Valider">
            <font>
                <Font size="12.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
